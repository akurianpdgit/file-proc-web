{
    "parser": "babel-eslint",
    "extends": "react",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-console": [
            0
        ]
    }
}